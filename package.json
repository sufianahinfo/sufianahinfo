{
  "name": "sufianah-clone",
  "version": "1.0.0",
  "description": "Sufianah Fragrances E-commerce Website",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": [
    "ecommerce",
    "fragrances",
    "perfumes",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "Sufianah Team",
  "license": "MIT",
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "axios": "^1.12.2",
    "jwt-decode": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@vitejs/plugin-react": "^5.0.0",
    "vite": "^7.1.2",
    "eslint": "^9.33.0",
    "tailwindcss": "^4.1.13",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6"
  }
}
